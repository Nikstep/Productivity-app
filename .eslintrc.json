{
"extends": [
    "airbnb",
    "prettier"
],
"plugins": [
    "prettier"
],
"parserOptions": {
    "ecmaVersion": 6
},
"env": {
    "node": true,
    "browser": true,
    "es6": true
},
"rules": {
    "prettier/prettier": "error",
    "comma-dangle": ["error", "always-multiline"],
    "no-plusplus":"off",
    "no-use-before-define":"off"
  }
}